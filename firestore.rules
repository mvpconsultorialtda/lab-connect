rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // TEMPORARY: Allow read/write for all authenticated users
    // This is to allow the first Admin to be created and content to be seeded.
    // You should revert this to more secure rules before going to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
